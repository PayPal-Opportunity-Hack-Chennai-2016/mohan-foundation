            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".FaqKidney"
            android:label="@string/title_activity_faq_kidney"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".FaqOrganDonation"
            android:label="@string/title_activity_faq_organ_doantion"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".StartScreen"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EyeBankSearch" />
        <activity
            android:name=".BecomeAMember"
            android:label="@string/title_activity_become_amember"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".SkinBankSearch" />
        <activity android:name=".DonationBody" />
        <activity android:name=".AddComments" />
        <activity android:name=".NGOSearch" />
        <activity android:name=".DonationHospitalSearch" />
